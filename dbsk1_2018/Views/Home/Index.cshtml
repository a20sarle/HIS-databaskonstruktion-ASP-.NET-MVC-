@{
    ViewBag.Title = "Index";
}
<h2>Önskelistor</h2>
<table>
    <tr>
        @foreach (var dataColumn in ViewBag.onskelista.Columns)
        {
            <th>@dataColumn.ColumnName</th>
        }
    </tr>
    @foreach (var customerRow in ViewBag.onskelista.Rows)
    {
        <tr>
            @for (int i = 0; i < ViewBag.onskelista.Columns.Count; ++i)
            {
                <td>@customerRow[i]</td>
            }
        </tr>
    }
</table>

